{
    "name": "bluespec-system-verilog",
    "displayName": "Bluespec System Verilog",
    "description": "Bluespec System Verilog support for VS Code adds syntax highlighting and snippets to help you.",
    "version": "1.0.0",
    "publisher": "Raamakrishnan A.",
    "homepage": "https://github.com/Raamakrishnan/bsv-for-vscode",
    "bugs": {
        "url": "https://github.com/Raamakrishnan/bsv-for-vscode/issues"
    },
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Raamakrishnan/bsv-for-vscode"
    },
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "BluespecSystemVerilog",
                "aliases": [
                    "Bluespec System Verilog",
                    "BSV",
                    "bsv"
                ],
                "extensions": [
                    ".bsv",
                    ".BSV"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "BluespecSystemVerilog",
                "scopeName": "source.bsv",
                "path": "./syntaxes/bluespec-system-verilog.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "BluespecSystemVerilog",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}